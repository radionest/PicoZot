{
  "name": "picozot",
  "version": "1.0.0-alpha",
  "description": "A Zotero plugin for PICO analysis and literature review generation",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack --config build/webpack.config.js",
    "dev": "webpack --config build/webpack.config.js --mode development --watch",
    "package": "node build/package.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "zotero",
    "plugin",
    "pico",
    "literature-review",
    "medical-research"
  ],
  "author": "Denis Nesterov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/radionest/picozot.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "core-js": "^3.31.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "firefox >= 60"
  ]
}
